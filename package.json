{
    "name": "info-considered-harmful",
    "version": "0.0.1",
    "description": "A template node typescript project",
    "main": "index.js",
    "engines": {
        "npm": ">=9.8.1",
        "node": ">=v20.5.1"
    },
    "scripts": {
        "prepare": "husky install",
        "build": "rimraf dist && tsc",
        "dev": "nodemon -e ts  --exec \"npm run build && node dist/index.js\"",
        "test": "jest --coverage",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' './*.js' './*.json' '!./package-lock.json'",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pjgrenyer/info-considered-harmful.git"
    },
    "keywords": [
        "node",
        "typescript"
    ],
    "author": "Paul Grenyer",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/pjgrenyer/info-considered-harmful/issues"
    },
    "homepage": "https://github.com/pjgrenyer/info-considered-harmful#readme",
    "dependencies": {
        "express": "^4.19.2",
        "on-finished": "^2.4.1",
        "tripitaka": "^5.0.2",
        "typescript": "^5.4.5",
        "yup": "^1.4.0"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.24.5",
        "@babel/preset-typescript": "^7.24.1",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/on-finished": "^2.3.4",
        "@types/supertest": "^2.0.16",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^8.57.0",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-mock-extended": "^3.0.7",
        "nock": "^13.5.4",
        "nodemon": "^3.1.0",
        "prettier": "^3.2.5",
        "supertest": "^6.3.4"
    }
}
